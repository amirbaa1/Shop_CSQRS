{
  "Routes": [
    {
      "DownstreamPathTemplate": "/api/basket/{UserId}",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": "7003"
        }
      ],
      "UpstreamPathTemplate": "/basket/{UserId}",
      "UpstreamHttpMethod": [
        "GET"
      ]
    },

    {
      "DownstreamPathTemplate": "/api/basket?UserId={userId}",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": "7003"
        }
      ],
      "UpstreamPathTemplate": "/basket?UserId={userId}",
      "UpstreamHttpMethod": [
        "POST"
      ]
    },

    {
      "DownstreamPathTemplate": "/api/basket",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": "7003"
        }
      ],
      "UpstreamPathTemplate": "/basket",
      "UpstreamHttpMethod": [
        "POST"
      ]
    },

    {
      "DownstreamPathTemplate": "/api/Basket?basketId={basketId}",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": "7003"
        }
      ],
      "UpstreamPathTemplate": "/Basket?basketId={basketId}",
      "UpstreamHttpMethod": [
        "DELETE"
      ]
    },

    {
      "DownstreamPathTemplate": "/api/Basket?BasketItemId={BasketItemId}&Quantity={Quantity}",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": "7003"
        }
      ],
      "UpstreamPathTemplate": "/Basket?BasketItemId={BasketItemId}&Quantity={Quantity}",
      "UpstreamHttpMethod": [
        "PUT"
      ]
    },

    {
      "DownstreamPathTemplate": "/api/Basket/sent",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": "7003"
        }
      ],
      "UpstreamPathTemplate": "/Basket/sent",
      "UpstreamHttpMethod": [
        "POST"
      ]
    }


  ],
  "GlobalConfiguration": {
    "BaseUrl": "https//localhost:7070"
  }
}